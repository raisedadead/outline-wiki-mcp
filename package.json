{
  "name": "outline-wiki-mcp",
  "version": "1.0.0",
  "description": "MCP server for Outline wiki integration",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "outline-wiki-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/raisedadead/outline-wiki-mcp.git"
  },
  "homepage": "https://github.com/raisedadead/outline-wiki-mcp#readme",
  "bugs": {
    "url": "https://github.com/raisedadead/outline-wiki-mcp/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test",
    "release": "semantic-release",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "outline",
    "wiki",
    "claude"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=22.14"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/exec": "^7.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/node": "^22.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^25.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
